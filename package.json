{
  "name": "static-web-page-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code to create a static web page",
  "main": "index.js",
  "scripts": {
    "scss": "sass assets/scss/main.scss assets/css/scss-output.css --watch",
