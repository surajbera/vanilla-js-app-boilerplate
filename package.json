  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.1.0",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.24",
    "postcss-cli": "^9.1.0",
    "postcss-sort-media-queries": "^5.2.0",
    "sass": "^1.62.1",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "browserslist": [
    "> .5% or last 2 versions"
  ],
  "dependencies": {}
